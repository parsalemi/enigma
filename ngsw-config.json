{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "index.html",
          "/*.css",
          "/*.js",
          "manifest.dark.json",
          "manifest.light.json",
          "favicon.ico"
        ]
      }
    },
    {
      "name": "public",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "icons/**",
          "fonts/**",
          "shuffle.json",
          "farsi-shuffle.json",
          "ByteBounce.ttf",
          "**/*.{svg,cur,jpg,jpeg,png,apng,webp,avif,gif,otf,ttf,woff,woff2}"
        ]
      }
    }
  ]
}


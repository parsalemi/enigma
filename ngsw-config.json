{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "index.html",
          "/*.css",
          "/*.js",
          "manifest.dark.json",
          "manifest.light.json",
          "favicon.ico"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "assets/**",
          "icons/**",
          "ByteBounce.ttf",
          "**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "rotors",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "shuffle.json",
          "farsi-shuffle.json",
          "icons/**",
          "**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ]
}

